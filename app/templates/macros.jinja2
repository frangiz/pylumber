{% macro price_table(data) %}
<div class="row">
    <div class="col-md-3"></div>
    <div class="col-md-6">
        <div class="card">
            <div class="card-body p-0">
                <table class="table table-sm">
                    <thead>
                        <tr>
                            <th>Store</th>
                            <th>Date</th>
                            <th>Price</th>
                            <th>Price changed</th>
                        </tr>
                    </thead>
                    <tbody>
                        {% for row in data %}
                        <tr>
                            <td>{{ row["store"] }}</td>
                            <td>{{ row["date"] }}</td>
                            <td>{{ row["price"] }}</td>
                            <td>{{ row["price_changed"] }}</td>
                        </tr>
                        {% endfor %}
                    </tbody>
                </table>
            </div> <!-- /.card-body -->
        </div>
    </div>
    <div class="col-md-3"></div>
</div>
{% endmacro %}}